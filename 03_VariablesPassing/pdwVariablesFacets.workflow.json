{
  "TYPE" : "Workflow",
  "version" : "1.1.0",
  "timeZone" : "Etc/UTC",
  "orderPreparation" : {
    "parameters" : {
      "business_date" : {
        "type" : "String",
        "default" : "\"13.03.2022\"",
        "facet" : "^(?:(?:31(\\/|-|\\.)(?:0?[13578]|1[02]))\\1|(?:(?:29|30)(\\/|-|\\.)(?:0?[13-9]|1[0-2])\\2))(?:(?:1[6-9]|[2-9]\\d)?\\d{2})$|^(?:29(\\/|-|\\.)0?2\\3(?:(?:(?:1[6-9]|[2-9]\\d)?(?:0[48]|[2468][048]|[13579][26])|(?:(?:16|[2468][048]|[3579][26])00))))$|^(?:0?[1-9]|1\\d|2[0-8])(\\/|-|\\.)(?:(?:0?[1-9])|(?:1[0-2]))\\4(?:(?:1[6-9]|[2-9]\\d)?\\d{2})$",
        "message" : "Enter business date dd.mm.yyyy, dd-mm-yyyy, for example\n* 12.03.2022\n* 12-03-2022"
      },
      "season_year_de" : {
        "type" : "String",
        "facet" : "^((Fr√ºhling)|(Sommer)|(Herbst)|(Winter)) [0-9][0-9][0-9][0-9]$",
        "message" : "Jahreszeit und Jahr angeben, z.B. \"Sommer 2022\", \"Herbst 2023\""
      },
      "season_year_en" : {
        "type" : "String",
        "default" : "\"Spring 2022\"",
        "facet" : "^((Spring)|(Summer)|(Autumn)|(Winter)) [0-9][0-9][0-9][0-9]$",
        "message" : "Enter season and year, for example. \"Summer 2022\", \"Autumn 2023\""
      },
      "flight_number" : {
        "type" : "String",
        "default" : "\"BA3456\"",
        "facet" : "^[A-Z]{2}\\d{4}$",
        "message" : "Enter flight number by 2 letter code followed by 4 digits, for example BA3456"
      }
    },
    "allowUndeclared" : false
  },
  "jobResourceNames" : [ "Default" ],
  "instructions" : [ {
    "TYPE" : "Execute.Named",
    "jobName" : "job1",
    "label" : "job1"
  } ],
  "jobs" : {
    "job1" : {
      "agentName" : "primaryAgent",
      "executable" : {
        "TYPE" : "ShellScriptExecutable",
        "script" : "#!/bin/bash\n\necho \"using workflow: $JS7_WORKFLOW_NAME\"\necho \"running job: $JS7_JOB_NAME\"\n\necho \"Business Date: $BUSINESS_DATE\"\necho \"Flight Number: $FLIGHT_NUMBER\"\necho \"Season and Year (English): $SEASON_YEAR_EN\"\n",
        "env" : {
          "BUSINESS_DATE" : "$business_date",
          "SEASON_YEAR_DE" : "$season_year_de",
          "SEASON_YEAR_EN" : "$season_year_en",
          "FLIGHT_NUMBER" : "$flight_number"
        },
        "v1Compatible" : false
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 500,
      "graceTimeout" : 15,
      "failOnErrWritten" : false
    }
  }
}