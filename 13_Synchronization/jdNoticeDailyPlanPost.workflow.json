{
  "TYPE" : "Workflow",
  "version" : "1.1.0",
  "timeZone" : "Etc/UTC",
  "title" : "Post notice to jdNoticeDailyPlan notice board",
  "jobResourceNames" : [ "Default" ],
  "instructions" : [ {
    "TYPE" : "Execute.Named",
    "jobName" : "job1",
    "label" : "job1"
  }, {
    "TYPE" : "PostNotice",
    "noticeBoardName" : "jdNoticeDailyPlan"
  }, {
    "TYPE" : "Execute.Named",
    "jobName" : "job2",
    "label" : "job2"
  } ],
  "jobs" : {
    "job1" : {
      "agentName" : "primaryAgent",
      "executable" : {
        "TYPE" : "ShellScriptExecutable",
        "script" : "#!/bin/bash\n\necho \"using workflow: $JS7_WORKFLOW_NAME\"\necho \"running job: $JS7_JOB_NAME\"\n\nsleep 10",
        "v1Compatible" : false
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 1,
      "graceTimeout" : 15,
      "failOnErrWritten" : false
    },
    "job2" : {
      "agentName" : "primaryAgent",
      "executable" : {
        "TYPE" : "ShellScriptExecutable",
        "script" : "#!/bin/bash\n\necho \"using workflow: $JS7_WORKFLOW_NAME\"\necho \"running job: $JS7_JOB_NAME\"\n\nsleep 5\n",
        "v1Compatible" : false
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 1,
      "graceTimeout" : 15,
      "failOnErrWritten" : false
    }
  }
}